{
  "printWidth": 120,
  "semi": true,
  "singleQuote": false,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "arrowParens": "avoid",
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "**/*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "tailwindStylesheet": "./src/styles/global.css"
}
